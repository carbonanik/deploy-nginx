pipeline {
    agent any
    stages {
        stage('verifing') {
            steps {
                sh '''
                docker version
                '''
            }
        }
        stage('start nginx') {
            steps {
                sh 'ls'
                sh 'cd deploy-nginx/'
                sh 'ls'
                sh 'docker compose up -d --build'
                sh 'docker compose ps'
            }
        }
    }
}